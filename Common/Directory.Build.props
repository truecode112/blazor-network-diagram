      <BaseIntermediateOutputPath>obj\hosted-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('Server_Common_NET7')) == 'true'">
      <BaseOutputPath>bin\server-net7\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net7\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('Wasm_Common_NET7')) == 'true'">
      <BaseOutputPath>bin\wasm-net7\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net7\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('Hosted_Common_NET7')) == 'true'">
