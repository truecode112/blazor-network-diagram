<Project>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('Server_Common_NET6')) == 'true'">
      <BaseOutputPath>bin\server-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('Wasm_Common_NET6')) == 'true'">
      <BaseOutputPath>bin\wasm-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net6\</BaseIntermediateOutputPath>
